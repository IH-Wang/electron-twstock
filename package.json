{
  "name": "electron-twstock",
  "version": "0.0.2",
  "description": "A Taiwan stock desktop application with Electron & Svelte",
  "main": "./main.js",
  "scripts": {
    "start": "run-p clean build:watch start:electron",
    "start:windows": "run-p clean:windows build:watch start:electron",
    "start:electron": "cross-env NODE_ENV=dev electron .",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "clean": "rm -rf ./public/build",
    "clean:windows": " if exist \"public/build\" rd /s /q \"public/build\"",
    "postinstall": "cross-env NODE_ENV=prod electron-builder install-app-deps",
    "package:forge": "electron-forge package",
    "make": "electron-forge make",
    "package": "electron-packager . --out=dist --ignore=src --ignore=rollup.config.js --ignore=.gitignore --overwrite",
    "package:windows": "node ./build.js",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte"
  },
  "repository": "https://github.com/IH-Wang/electron-twstock",
  "keywords": [
    "Electron",
    "Svelte",
    "stock",
    "demo"
  ],
  "author": "IH-Wang",
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-appx": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "prettier": "2.2.1",
    "prettier-plugin-svelte": "^1.4.1",
    "rollup": "^2.34.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "iconv-lite": "^0.6.2",
    "idb": "^5.0.8",
    "nedb-promises": "^4.1.0",
    "ramda": "^0.27.1",
    "svelte": "^3.31.0",
    "svelte-loading-spinners": "^0.1.1",
    "svelte-spa-router": "^3.1.0"
  }
}
