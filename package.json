{
  "name": "electron-svelte-demo",
  "version": "0.0.2",
  "description": "A Taiwan stock desktop application with Electron & Svelte",
  "main": "src/main.js",
  "scripts": {
    "start": "run-p clean build:watch start:electron",
    "start:windows": "run-p clean:windows build:watch start:electron",
    "start:electron": "cross-env NODE_ENV=dev electron .",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "clean": "rm -rf ./public/build",
    "clean:windows": " if exist \"public/build\" rd /s /q \"public/build\"",
    "postinstall": "cross-env NODE_ENV=prod electron-builder install-app-deps",
    "package": "electron-packager . --out=dist --ignore=src --ignore=rollup.config.js --ignore=.gitignore --overwrite",
    "package:windows": "node ./build.js",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,svelte}'",
    "lint": "eslint --fix --quiet src --ext .js,.svelte"
  },
  "repository": "https://github.com/IH-Wang/electron-svelte-demo",
  "keywords": [
    "Electron",
    "Svelte",
    "stock",
    "demo"
  ],
  "author": "IH-Wang",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "autoprefixer": "^9",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.1.0",
    "electron-reload": "^1.5.0",
    "electron-winstaller": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "postcss-load-config": "^3.0.0",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "^1.4.1",
    "rollup": "^2.33.3",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "dependencies": {
    "@electron/remote": "^1.0.0",
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "iconv-lite": "^0.6.2",
    "nedb-promises": "^4.1.0",
    "ramda": "^0.27.1",
    "svelte": "^3.30.0",
    "svelte-loading-spinners": "^0.1.1",
    "svelte-spa-router": "^3.0.5"
  }
}
